<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<OutputType>Library</OutputType>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Authors>Alexander Forbes Group Services (Pty) Ltd.</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/alexander-forbes/mapex</PackageProjectUrl>
		<RepositoryUrl>https://github.com/alexander-forbes/mapex.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<Description>A dynamic mapping engine to extract, transform and load data using YAML directives from various data sources.</Description>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
		<PackageReference Include="AutoMapper" Version="10.1.1" />
		<PackageReference Include="Notus" Version="2.0.0" />
		<PackageReference Include="System.Linq.Dynamic.Core" Version="1.2.13" />
		<PackageReference Include="YamlDotNet" Version="11.2.1" />
	</ItemGroup>
</Project>